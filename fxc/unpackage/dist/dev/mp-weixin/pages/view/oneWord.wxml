<view class="data-v-6cfd5b1f"><block wx:for="{{messages}}" wx:for-item="message" wx:for-index="index" wx:key="index"><view class="message data-v-6cfd5b1f"><view class="message-content data-v-6cfd5b1f"><view class="content data-v-6cfd5b1f">{{message.messageContent}}</view><view class="source data-v-6cfd5b1f"><text class="data-v-6cfd5b1f">{{message.messageDate+''}}</text><text class="data-v-6cfd5b1f">{{"出自“"+message.userNickname+"”"}}</text></view></view></view></block><view data-event-opts="{{[['tap',[['showInputDialog',['$event']]]]]}}" class="floating-button data-v-6cfd5b1f" bindtap="__e">+</view><block wx:if="{{showDialog}}"><view class="input-dialog data-v-6cfd5b1f"><view class="input-container data-v-6cfd5b1f"><textarea placeholder="请输入留言内容" data-event-opts="{{[['input',[['__set_model',['','newMessage','$event',[]]]]]]}}" value="{{newMessage}}" bindinput="__e" class="data-v-6cfd5b1f"></textarea><view class="button-container data-v-6cfd5b1f"><button data-event-opts="{{[['tap',[['publishMessage',['$event']]]]]}}" bindtap="__e" class="data-v-6cfd5b1f">发布</button><button data-event-opts="{{[['tap',[['cancelMessage',['$event']]]]]}}" bindtap="__e" class="data-v-6cfd5b1f">取消</button></view></view></view></block></view>