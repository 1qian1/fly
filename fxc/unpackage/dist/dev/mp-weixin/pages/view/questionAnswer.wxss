
.container {
  padding: 20rpx;
}
.question-item {
  margin-bottom: 20rpx;
  background-color: #f7f7f7;
  padding: 20rpx;
  border-radius: 10rpx;
  box-shadow: 0 2rpx 4rpx rgba(0, 0, 0, 0.1);
}
.question {
  font-size: 16px;
  font-weight: bold;
  color: #333;
}
.user-nickname {
  font-size: 14px;
  color: #666;
}
.info {
  display: flex;
  justify-content: space-between;
}

